    <hudson.tasks.Shell>
      <command>#!/bin/ksh

cd /opt/build/scripts

VERSION=`./check_dbversions.sh -a ads-core -e devint`

print Gathering build version to be deployed
print --------------------------------------
print
print Deploying version ${VERSION}
print
print
print
print Beginning Rebuild from scratch for db to latest package version ${VERSION}
print ------------------------------------------------------------------------------------------------------------
./kill_dbsessions.sh -a ads-core -e scrum1

cd /opt/checkouts/ads-core/core/model/src/main/resources/liquibase/cisys

echo &quot;@genUser.scrum1.sql;&quot; | sqlplus -s &apos;cm_system/oracle123@(DESCRIPTION=(ADDRESS_LIST=(ADDRESS=(PROTOCOL=TCP)(HOST=10.13.18.119)(PORT=1522)))(CONNECT_DATA=(SERVICE_NAME=scrum001)))&apos;

STATUS=${?}
if [[ ${STATUS} != &quot;0&quot; ]]; then
    exit ${STATUS}
fi

cd /opt/checkouts/oss_deployment/scripts

./deployRebuildDb.sh -a ads-core -t ads-core -i cm -r ${VERSION} -e scrum1 -j &quot;jdbc:oracle:thin:@10.13.18.119:1522:scrum001&quot; -p SC1_ADS -u SC1_ADS -v
</command>
    </hudson.tasks.Shell>
