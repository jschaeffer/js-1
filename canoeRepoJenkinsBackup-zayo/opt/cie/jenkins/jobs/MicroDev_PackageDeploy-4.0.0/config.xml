      <scriptSource class="hudson.plugins.groovy.StringScriptSource">
        <command>def VERSION = &quot;4.0.0&quot;
def PROJECT = &quot;MicroDev&quot;
def PROJFILE = &quot;/opt/build/scm/scripts/buildServer/properties/projects/$PROJECT/project.properties&quot;
String changelogtemp = &quot;&quot;&quot;

projectRoot=&quot;/opt/cie/jenkins/jobs/MicroDev_PackageDeploy-$VERSION/workspace&quot;
webappDir=&quot;webapps&quot;

build {
    build_args=&quot;&quot;
}

build_package {
    pre_pkg_cmds {
            pre_pkg_1=&quot;cd \$projectRoot; mv ProjectPackage.mmp MicroDev.mmp&quot;
    }
    artifacts {
        name {
            bin=&quot;MicroDev.mmp&quot;
        }
        location {
            bin=&quot;\$projectRoot&quot;
        }
    }
}

&quot;&quot;&quot;
File scriptName = new File(&quot;$PROJFILE&quot;)
        scriptName.write(&quot;${changelogtemp}&quot;)
        println scriptName</command>
      </scriptSource>
      <groovyName>(Default)</groovyName>
