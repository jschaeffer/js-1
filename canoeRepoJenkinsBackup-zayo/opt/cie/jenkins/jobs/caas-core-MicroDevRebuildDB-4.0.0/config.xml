print Gathering build version to be deployed
print --------------------------------------
print
print Deploying version ${VERSION}
print
print
print
print Beginning Rebuild from scratch for db to latest package version ${VERSION}
print ------------------------------------------------------------------------------------------------------------

./kill_dbsessions.sh -a caas-core -e MicroDev

cd /opt/checkouts/caas-core/core/model/src/main/resources/liquibase/cisys

echo &quot;@genUser.MicroDev.sql;&quot; | sqlplus -s &apos;cm_system/oracle123@(DESCRIPTION=(ADDRESS_LIST=(ADDRESS=(PROTOCOL=TCP)(HOST=10.13.18.61)(PORT=1522)))(CONNECT_DATA=(SERVICE_NAME=scrum004)))&apos;

STATUS=${?}
if [[ ${STATUS} != &quot;0&quot; ]]; then
    exit ${STATUS}
fi

cd /opt/checkouts/oss_deployment/scripts

./MicroDevDB.sh -a caas-core -t caas-core -i cm -r ${VERSION} -e MicroDev -j &quot;jdbc:oracle:thin:@10.13.18.61:1522:scrum004&quot; -p DI4_CAAS -u DI4_CAAS -v
</command>
