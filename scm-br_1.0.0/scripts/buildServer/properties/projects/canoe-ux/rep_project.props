          }
          location {
            war="$commonTargetRoot/"
          }
          deploy_type {
            war="archive"
          }
          webapp_project {
            war="dai_reporting"
          }
         }

        environment {
            name="dvappdai01"
            ip="10.13.18.113"
        }

        tcserver {
           base="/opt/tcserver/vfabric-tc-server-standard-2.6.4.RELEASE"
